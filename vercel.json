{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@furntech-mongodb-uri",
    "SECRET_KEY": "@furntech-secret-key",
    "FRONTEND_URL": "https://radhedispoworld.netlify.app/"
  }
}